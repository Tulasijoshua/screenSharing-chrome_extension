{
    "name": "HelpMeOut",
    "description": "Open you go to tabs in one click",
    "version": "1.0",
    "manifest_version": 3,

    "action": {
        "default_title": "HelpMeOut",
        "default_popup": "index.html",
        "default_icon": {
            "16": "logo.png",
            "128": "logo.png"
        }
    },

    
    "permissions": [
        "scripting"
    ],

    "host_permissions": [
        "https://*/",
        "http://*/"
    ]

}